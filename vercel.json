{
    "rewrites": [
        {
            "source": "/backend/scholar",
            "destination": "/api/scholar"
        },
        {
            "source": "/backend/_update_scholar",
            "destination": "/api/update_scholar"
        },
        {
            "source": "/backend/(.*)",
            "destination": "/api/proxy"
        },
        {
            "source": "/:path*",
            "destination": "/index.html"
        }
    ]
}
